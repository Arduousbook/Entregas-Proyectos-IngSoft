<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="Main" alignment="CENTER" spacing="20.0" stylesheets="@style_1.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javeriana.Study_With_Me.controller.ConfigureProfile_windowController">
      <Label id="title" fx:id="title" text="Configurar Perfil" />
   <HBox alignment="CENTER_LEFT" maxWidth="820.0" prefWidth="820.0">
      <children>
         <Label id="room1" text="Nombre:" />
      </children>
   </HBox>
      <TextField fx:id="fillName" maxWidth="820.0" onAction="#profileName" prefWidth="820.0" />
   <Label id="warning" fx:id="warning" />
      <Button fx:id="continueButton" minWidth="150.0" mnemonicParsing="false" onAction="#onClickContinue" text="Continuar" />
    <Button fx:id="returnButton" minWidth="150.0" mnemonicParsing="false" onAction="#onClickBack" text="Volver" />
</VBox>
